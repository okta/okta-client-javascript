{
  "name": "@okta/spa-platform",
  "version": "0.4.1",
  "type": "module",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "private": true,
  "files": [
    "./dist",
    "*.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js"
    },
    "./fetch": {
      "types": "./dist/types/FetchClient/index.d.ts",
      "import": "./dist/esm/FetchClient/index.js"
    },
    "./orchestrator": {
      "types": "./dist/types/TokenOrchestrator/index.d.ts",
      "import": "./dist/esm/TokenOrchestrator/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.jsx .",
    "build": "yarn build:esm && yarn build:types",
    "build:watch": "yarn build:esm --watch & yarn build:types --watch",
    "build:esm": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "yarn test:unit",
    "test:watch": "jest --watchAll",
    "test:unit": "jest"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@okta/auth-foundation": "*",
    "@okta/spa-oauth2-flows": "*"
  },
  "peerDependenciesMeta": {
    "@okta/spa-oauth2-flows": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tsd/typescript": "^5.0.3",
    "@types/node": "^18.15.3",
    "@types/webcrypto": "^0.0.28",
    "@repo/eslint-config": "*",
    "@repo/jest-helpers": "*",
    "@repo/typescript-config": "*",
    "@repo/rollup-config": "*",
    "@okta/auth-foundation": "*",
    "@okta/spa-oauth2-flows": "*",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rollup": "^4.18.0",
    "ts-node": "^10.9.2",
    "tsd": "^0.28.1",
    "typescript": "^5.4.5"
  }
}