{
  "name": "@okta/auth-foundation",
  "version": "0.4.2",
  "type": "module",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "jared.perreault@okta.com",
  "license": "Apache-2.0",
  "private": true,
  "files": [
    "./dist",
    "*.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js"
    },
    "./client": {
      "types": "./dist/types/client.d.ts",
      "import": "./dist/esm/client.js"
    },
    "./internal": {
      "types": "./dist/types/internal.d.ts",
      "import": "./dist/esm/internal.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "./src/oktaUserAgent.ts"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts,.jsx .",
    "build": "yarn build:esm && yarn build:types",
    "build:watch": "rollup -c --watch & tsc --emitDeclarationOnly --watch",
    "build:esm": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "yarn test:unit",
    "test:watch": "jest --watchAll",
    "test:unit": "jest"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/rollup-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^18.15.3",
    "eslint": "^8.56.0",
    "rollup": "^4.18.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
