{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@wdio",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "node",
      "expect-webdriverio"
    ]
  }
}
